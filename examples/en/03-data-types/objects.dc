# Example 9: Using objects as dictionaries
print("üîç Example 9: Using objects as dictionaries...")
translations = {
    'hello': 'Hola',
    'goodbye': 'Adi√≥s',
    'thank you': 'Gracias',
    'please': 'Por favor'
}

fn translate(word) {
    try {
        return translations[word]
    } catch KeyError {
        return word
    }
}

print("  ‚úì Translation dictionary:")
print("    translate('hello') =", translate('hello'))
print("    translate('goodbye') =", translate('goodbye'))
print("    translate('unknown') =", translate('unknown'))
print()

