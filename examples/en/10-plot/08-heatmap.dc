import plot

data = [
  [1, 2, 3, 4, 5],
  [2, 4, 6, 8, 10],
  [3, 6, 9, 12, 15],
  [4, 8, 12, 16, 20],
  [5, 10, 15, 20, 25]
]

plot.heatmap(data)
plot.show(title="heat matrix")

plot.heatmap(data, min = 0, max = 100)
plot.show(title="2heat matrix")

plot.heatmap(data, palette = "red")
plot.show(title="3heat matrix")


