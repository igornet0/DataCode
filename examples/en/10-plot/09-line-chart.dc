import plot

x = [1,2,3,4,5]
y = [2,4,1,6,3]
y1 = [7,12,2,9,1]
y2 = [1,3,1,10,20]

plot.xlabel("Time")
plot.ylabel("Value")

plot.line(x, y, true, color="blue")
plot.line(x, y1, true, color="green") # blue, green, red, black, white 
plot.line(x, y2, true, color="#a434eb")
plot.show(title="My Chart")

