    'thank you': '–°–ø–∞—Å–∏–±–æ',
    'please': '–ü–æ–∂–∞–ª—É–π—Å—Ç–∞'
}

fn translate(word) {
    if translations[word] != null {
        return translations[word]
    } else {
        return word
    }
}

